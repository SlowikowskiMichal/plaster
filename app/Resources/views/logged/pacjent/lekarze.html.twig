        {% for lekarz in pacjentList %}
            <div class="container">
                <p>
                <center>
                    <h2><b>Lekarz: {{ lekarz.getId }}</b></h2>
                    <b>ImiÄ™:</b> {{ lekarz.getImie }}<br>
